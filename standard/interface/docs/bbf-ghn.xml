<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <interfaces xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
    <interface>
      <name/>
      <description/>
      <type/>
      <enabled>True</enabled>
      <link-up-down-trap-enable/>
      <ghn-interface xmlns="urn:bbf:yang:bbf-ghn">
        <domain>
          <no-profiles-attached/>
          <scheduling-profile/>
          <security-profile/>
          <dba-profile/>
          <domain-config-vector/>
        </domain>
        <node>
          <node-mac-address/>
          <node>
            <no-profiles-attached/>
            <node-id-profile/>
            <node-config-vector/>
          </node>
          <link>
            <device-id/>
            <no-profiles-attached/>
            <line-spectrum-profile/>
            <channel-estimation-profile/>
            <link-config-vector/>
          </link>
        </node>
      </ghn-interface>
    </interface>
  </interfaces>
  <interfaces-state xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
    <interface>
      <name/>
      <type/>
      <admin-status/>
      <oper-status/>
      <last-change/>
      <if-index/>
      <phys-address/>
      <higher-layer-if/>
      <lower-layer-if/>
      <speed/>
      <statistics>
        <discontinuity-time/>
        <in-octets/>
        <in-unicast-pkts/>
        <in-broadcast-pkts/>
        <in-multicast-pkts/>
        <in-discards/>
        <in-errors/>
        <in-unknown-protos/>
        <out-octets/>
        <out-unicast-pkts/>
        <out-broadcast-pkts/>
        <out-multicast-pkts/>
        <out-discards/>
        <out-errors/>
      </statistics>
      <ghn-interface xmlns="urn:bbf:yang:bbf-ghn">
        <domain>
          <status>
            <domain-scheduling>
              <mac-cycle-duration/>
            </domain-scheduling>
            <domain-information>
              <domain-name/>
              <dni/>
              <minimum-supported-bandplan/>
              <maximum-supported-bandplan/>
            </domain-information>
            <domain-topology>
              <topology-update-interval/>
            </domain-topology>
            <security>
              <encryption-enabled>True</encryption-enabled>
              <target-domains/>
            </security>
          </status>
        </domain>
        <node>
          <node-mac-address/>
          <node>
            <status>
              <is-dm/>
              <device-id/>
              <dm-capable/>
              <sc-capable/>
              <bandplan/>
              <is-sc/>
              <medium/>
              <medium-access-type/>
              <up-time/>
              <boot-count/>
              <last-forced-resignation>
                <event-time/>
                <event-cause/>
              </last-forced-resignation>
              <last-unsuccessful-registration>
                <event-time/>
                <event-cause/>
              </last-unsuccessful-registration>
              <aat>
                <laat>
                  <mac-entry/>
                  <mac-type/>
                </laat>
                <raat>
                  <mac-entry/>
                  <mac-type/>
                </raat>
              </aat>
            </status>
            <inventory>
              <certified-vendor-id/>
              <certification-version/>
              <self-test-passed/>
              <system-serial-number/>
              <node-mac-address/>
              <software-version/>
              <hardware-version/>
              <manufacturer-oui/>
              <model-name/>
            </inventory>
            <performance>
              <intervals-15min>
                <current>
                  <measured-time/>
                  <initializations/>
                  <master-domain-losses/>
                  <maps-lost/>
                  <registrations/>
                  <re-registrations/>
                  <transmission-queue-overflow/>
                </current>
                <number-of-intervals/>
                <non-valid-intervals/>
                <history>
                  <interval-number/>
                  <measured-time/>
                  <invalid-data-flag/>
                  <time-stamp/>
                  <initializations/>
                  <master-domain-losses/>
                  <maps-lost/>
                  <registrations/>
                  <re-registrations/>
                  <transmission-queue-overflow/>
                </history>
              </intervals-15min>
              <intervals-24hr>
                <current>
                  <measured-time/>
                  <initializations/>
                  <master-domain-losses/>
                  <maps-lost/>
                  <registrations/>
                  <re-registrations/>
                  <transmission-queue-overflow/>
                </current>
                <number-of-intervals/>
                <non-valid-intervals/>
                <history>
                  <interval-number/>
                  <measured-time/>
                  <invalid-data-flag/>
                  <time-stamp/>
                  <initializations/>
                  <master-domain-losses/>
                  <maps-lost/>
                  <registrations/>
                  <re-registrations/>
                  <transmission-queue-overflow/>
                </history>
              </intervals-24hr>
            </performance>
          </node>
          <link>
            <device-id/>
            <status>
              <rates>
                <maximum-bit-rate/>
                <out-estimated-phy-rate/>
                <in-estimated-phy-rate/>
                <out-phy-rate/>
                <in-phy-rate/>
              </rates>
              <channel>
                <sub-carrier-grouping/>
                <port>
                  <port-number/>
                  <tidx-min/>
                  <tidx-max/>
                  <bpc/>
                </port>
                <spectrum>
                  <sub-carrier-mask>
                    <notch-number/>
                    <lowest-index/>
                    <highest-index/>
                  </sub-carrier-mask>
                  <radio-amateur-mask>
                    <notch-number/>
                    <lowest-index/>
                    <highest-index/>
                  </radio-amateur-mask>
                </spectrum>
              </channel>
            </status>
            <performance>
              <intervals-15min>
                <current>
                  <measured-time/>
                  <initializations/>
                  <out-management-bytes/>
                  <out-management-packets/>
                  <in-valid-phy-headers/>
                  <in-invalid-phy-headers/>
                  <in-phy-frames/>
                  <out-phy-frames/>
                  <in-lpdu-frames/>
                  <out-lpdu-frames/>
                  <out-resent-lpdu-frames/>
                  <in-crc-errored-lpdu-frames/>
                  <in-llc-frames/>
                  <out-llc-frames/>
                  <in-crc-errored-llc-frames/>
                  <in-mic-errored-llc-frames/>
                </current>
                <number-of-intervals/>
                <non-valid-intervals/>
                <history>
                  <interval-number/>
                  <measured-time/>
                  <invalid-data-flag/>
                  <time-stamp/>
                  <initializations/>
                  <out-management-bytes/>
                  <out-management-packets/>
                  <in-valid-phy-headers/>
                  <in-invalid-phy-headers/>
                  <in-phy-frames/>
                  <out-phy-frames/>
                  <in-lpdu-frames/>
                  <out-lpdu-frames/>
                  <out-resent-lpdu-frames/>
                  <in-crc-errored-lpdu-frames/>
                  <in-llc-frames/>
                  <out-llc-frames/>
                  <in-crc-errored-llc-frames/>
                  <in-mic-errored-llc-frames/>
                </history>
              </intervals-15min>
              <intervals-24hr>
                <current>
                  <measured-time/>
                  <initializations/>
                  <out-management-bytes/>
                  <out-management-packets/>
                  <in-valid-phy-headers/>
                  <in-invalid-phy-headers/>
                  <in-phy-frames/>
                  <out-phy-frames/>
                  <in-lpdu-frames/>
                  <out-lpdu-frames/>
                  <out-resent-lpdu-frames/>
                  <in-crc-errored-lpdu-frames/>
                  <in-llc-frames/>
                  <out-llc-frames/>
                  <in-crc-errored-llc-frames/>
                  <in-mic-errored-llc-frames/>
                </current>
                <number-of-intervals/>
                <non-valid-intervals/>
                <history>
                  <interval-number/>
                  <measured-time/>
                  <invalid-data-flag/>
                  <time-stamp/>
                  <initializations/>
                  <out-management-bytes/>
                  <out-management-packets/>
                  <in-valid-phy-headers/>
                  <in-invalid-phy-headers/>
                  <in-phy-frames/>
                  <out-phy-frames/>
                  <in-lpdu-frames/>
                  <out-lpdu-frames/>
                  <out-resent-lpdu-frames/>
                  <in-crc-errored-lpdu-frames/>
                  <in-llc-frames/>
                  <out-llc-frames/>
                  <in-crc-errored-llc-frames/>
                  <in-mic-errored-llc-frames/>
                </history>
              </intervals-24hr>
            </performance>
          </link>
        </node>
        <local-node-info>
          <local-node-mac/>
        </local-node-info>
      </ghn-interface>
    </interface>
  </interfaces-state>
  <ghn xmlns="urn:bbf:yang:bbf-ghn">
    <domain>
      <scheduling-profile>
        <name/>
        <mac-cycle-duration>40</mac-cycle-duration>
        <split-ratio>50</split-ratio>
        <topology-update-interval/>
      </scheduling-profile>
      <security-profile>
        <name/>
        <encryption-enabled>True</encryption-enabled>
        <encryption-mode>nmk</encryption-mode>
        <mic-size/>
        <target-domains/>
      </security-profile>
      <dba-profile>
        <name/>
        <split-ratio/>
      </dba-profile>
      <domain-id-profile>
        <name/>
        <domain-name/>
        <minimum-supported-bandplan/>
        <maximum-supported-bandplan/>
      </domain-id-profile>
    </domain>
    <node>
      <node-id-profile>
        <name/>
        <force-dm/>
        <force-sc/>
        <bandplan/>
        <medium>coaxial</medium>
        <medium-access-type>siso</medium-access-type>
      </node-id-profile>
    </node>
    <link>
      <line-spectrum-profile>
        <name/>
        <sub-carrier-mask>
          <notch-number/>
          <lowest-index/>
          <highest-index/>
        </sub-carrier-mask>
        <radio-amateur-mask>
          <notch-number/>
          <lowest-index/>
          <highest-index/>
        </radio-amateur-mask>
      </line-spectrum-profile>
      <channel-estimation-profile>
        <name/>
        <minimum-grouping-size/>
      </channel-estimation-profile>
    </link>
    <vectors>
      <domain-config-vector>
        <name/>
        <scheduling-profile/>
        <security-profile/>
        <dba-profile/>
      </domain-config-vector>
      <node-config-vector>
        <name/>
        <node-id-profile/>
      </node-config-vector>
      <link-config-vector>
        <name/>
        <line-spectrum-profile/>
        <channel-estimation-profile/>
      </link-config-vector>
    </vectors>
  </ghn>
</data>
